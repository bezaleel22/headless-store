{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently bun:dev:*",
    "build": "bun --bun src/generate.ts && tsc",
    "generate": "bun --bun dist/generate.js",
    "start:server": "bun --bun ./dist/index.js",
    "start:worker": "bun --bun ./dist/index-worker.js",
    "start": "concurrently bun:start:*"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "2.2.0",
    "@vendure/asset-server-plugin": "2.2.0",
    "@vendure/core": "2.2.0",
    "@vendure/email-plugin": "2.2.0",
    "dotenv": "16.4.5",
    "pg": "8.11.5",
    "typescript": "5.3.3"
  },
  
  "devDependencies": {
    "@vendure/cli": "2.2.0",
    "@vendure/ui-devkit": "^2.2.0",
    "concurrently": "8.2.2"
  }
}
